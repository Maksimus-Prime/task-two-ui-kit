mixin map-block(id,lat,lng,zoom,marker)
	.map-block
		.map(data-lat=lat+"" data-lng=lng+"" data-zoom=zoom+"" data-marker=marker+"")
		.map-block-description
			h1="Meet us!"
			p="1259 California st San Francisco, CA"
			.map-block-description__icons
				i.fa.fa-dot-circle-o(aria-hidden="true")
				i.fa.fa-map-marker(aria-hidden="true")
