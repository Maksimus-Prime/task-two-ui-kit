mixin map-block(id,lat,lng,zoom,marker)
    .map-block
        .map(class="js-map" data-lat=lat+"" data-lng=lng+"" data-zoom=zoom+"" data-marker=marker+"")
        .map-block-info
            h3.map-block-info__title="Meet us!"
            p.map-block-info__description="1259 California st San Francisco, CA"
            .map-block-info__icons
                i.fa.fa-dot-circle-o(aria-hidden="true")
                i.fa.fa-map-marker(aria-hidden="true")